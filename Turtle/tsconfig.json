{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "target": "es5",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "plugins": [
      {
        "transform": "ts-auto-mock/transformer",
        "cacheBetweenTests": false
      }
    ]
  }
}
